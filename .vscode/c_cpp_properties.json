{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "/usr/include/**",
                "/usr/local/bin/hdf5/include",
                "/usr/local/bin/netcdf/include",
                "/usr/local/lib/boost/include",
                "/usr/local/bin/trilinos/include",
                "/usr/local/lib/openmpi/include",
                "${workspaceFolder}/src/**"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu17",
            "cppStandard": "gnu++14",
            "intelliSenseMode": "gcc-x64",
            "compilerArgs": [
                "-D CMAKE_BUILD_TYPE:STRING=Release",
                "-D Trilinos_DIR:PATH=/usr/local/bin/trilinos/lib/cmake/Trilinos/",
                "-D CMAKE_C_COMPILER:STRING=/usr/local/lib/openmpi/bin/mpicc",
                "-D CMAKE_CXX_COMPILER:STRING=/usr/local/lib/openmpi/bin/mpicxx",
                "-D BOOST_ROOT=/usr/local/lib/boost/",
                "-D CMAKE_CXX_FLAGS:STRING=\"-O2 -Wall -std=c++11 -pedantic -Wno-long-long -ftrapv -Wno-deprecated-declarations\""
            ],
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}